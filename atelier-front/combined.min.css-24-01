.page { background-size: cover; background-position: center; will-change: transform; position: relative; top: 0; left: 0; width: 100% }
.transition.animate-in-enter-complete { position: relative }
@media (min-width:1025px) {
.page { height: 100%; position: absolute!important; opacity: 0; -webkit-transform: translateY(100%); transform: translateY(100%); -webkit-transition: -webkit-transform ease-in-out .5s!important; transition: transform ease-in-out .5s!important }
.page-next, .page-selected, .page-selected-add, .page-selected-remove { opacity: 1 }
.page-selected{ background:none !important; }


.page.ng-show { -webkit-transform: translateY(0); transform: translateY(0) }
.pager-chevron-container { cursor: pointer; position: absolute; left: 0; right: 0; top: 100%; margin: auto; opacity: 0; width: 100%; height: 24px; color: #fff; -webkit-transition: all ease-in-out .5s; transition: all ease-in-out .5s }
.pager-chevron { height: 100%; background-image: url(assets/frontend/images/chevron.svg); background-repeat: no-repeat; background-position: center center; color: #fff; -webkit-transition: all ease-in-out .5s; transition: all ease-in-out .3s }
.page-peak+.pager-chevron-container { opacity: 1; -webkit-transform: translateY(-45px); transform: translateY(-45px); animation-timing-function: ease-in-out; animation-fill-mode: forewards }
.pager-chevron-container:hover .pager-chevron { transform: translateY(-10px) }
.pager-footer { position: absolute; bottom: 0; left: 0; height: 0; width: 100%; overflow: hidden; z-index: 5; -webkit-transition: height ease-in-out .5s; transition: height ease-in-out .5s }
.page-footer, .pager-show-footer.pager-footer-focus .pager-footer { height: 100% }
.pager-show-footer .pager-footer { height: 50% }
.pager-show-footer .page.ng-show { transition: transform ease-in-out .5s; -webkit-transform: translate3d(0, calc(-50% + 6px), 0); transform: translate3d(0, calc(-50% + 6px), 0); -ms-transform: translate3d(0, -45%, 0) }
.pager .mapHolder .city, .pager .mapHolder .worldMap { opacity: 0 }
.pager .pager-show-footer .pager-footer .city { display: none }
.pager .pager-show-footer.pager-footer-focus .pager-footer .city { display: block }
.pager .pager-footer-focus-complete .city { opacity: 1 }
.pager .pager-show-footer.pager-footer-focus .pager-footer .mapHolder { height: auto }
.pager .pager-show-footer.pager-footer-focus .pager-footer .mapHolder .worldMap { display: block; opacity: 1 }
.pager .pager-show-footer .pager-footer .mapTitle { opacity: 1 }
.pager .pager-show-footer .pager-footer .mapInfo { opacity: 1; bottom: 136px }
.pager .pager-show-footer.pager-footer-focus .pager-footer .mapInfo { bottom: 100px }
.pager .pager-footer>div, .pager-footer>div>div { height: 100% }
.pager .pager-show-footer.pager-footer-focus .aspectMap { opacity: 1 }
.pager footer { position: absolute; width: 100% }
footer { bottom: 0; }
.ng-leave footer { -webkit-transition: all 0s ease-in-out!important; -moz-transition: all 0s ease-in-out!important; -ms-transition: all 0s ease-in-out!important; -o-transition: all 0s ease-in-out!important; transition: all 0s ease-in-out!important; -webkit-transition-delay: 0s!important; -moz-transition-delay: 0s!important; -ms-transition-delay: 0s!important; -o-transition-delay: 0s!important; transition-delay: 0s!important; visibility: hidden }
.pager .pager-show-footer footer { bottom: 0; }
}
